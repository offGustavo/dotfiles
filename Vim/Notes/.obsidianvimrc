". Yank to system clipboard
set clipboard=unnamedplus

" Go back and forward with Ctrl+O and Ctrl+I
" (make sure to remove default Obsidian shortcuts for these to work)
exmap go-back obcommand app:go-back
nmap <C-o> :go-back<CR>
exmap go-forward obcommand app:go-forward
nmap <C-i> :go-forward<CR>

exmap toggle-bold obcommand editor:toggle-bold
vmap <C-b> :toggle-bold<Cr>
exmap toggle-italics obcommand editor:toggle-italics
vmap <C-i> :toggle-italics<Cr>

" Unmap
" unmap <C-c>

" " Navgation
" nmap h <Left>
" nmap j <Down>
" nmap k <Up>
" nmap l <Right>


" Best Navgation
nmap j gj
nmap k gk

" Fold and Unfold
exmap unfoldall obcommand editor:unfold-all
nmap zR :unfoldall<Cr>

exmap foldall obcommand editor:fold-all
nmap zM :foldall<Cr>

exmap foldtoggle obcommand editor:toggle-fold
nmap za :foldtoggle<Cr>

" Buffers 
exmap ntab obcommand workspace:next-tab
nmap ]b :ntab<Cr>
exmap ptab obcommand workspace:previous-tab
nmap [b :ptab<Cr>

" Vim File Explorer
exmap Ex obcommand file-explorer:reveal-active-file
nmap \e :Ex<Cr>

" Quit File

" " Better Navigation
" nmap <C-d> <C-d>zz
" nmap <C-u> <C-u>zz

