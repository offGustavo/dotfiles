    "interval": 1,
    "critical-threshold": 80,
    "on-click": "kitty -e btop",
  },
  "custom/powerDraw": {
    "format": "{}",
    "interval": 1,
    "exec": "~/.config/waybar/scripts/powerdraw.sh",
    "return-type": "json"
  },
  "niri/workspaces": {
	"format": "{icon}",
	"format-icons": {
		// Named workspaces
		// (you need to configure them in niri)
		"browser": "ÓÄá",
		"discord": "Ôéí",
		"chat": "<b>ÔÅµ</b>",
		// Icons by state
		"active": "ÔÜí",
		"default": "ÔÑë"
	}
},
  "hyprland/workspaces": {
    // "format": "<span font='18px'>{icon}</span>",
    "format": "{icon}",
    // "format-icons": {
    //   "1": "‡•ß",
